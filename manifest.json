{
    "manifest_version": 3,
    "name": "Linkedin Bot",
    "description": "Base Level Extension",
    "version": "1.0",
    "background.service_worker": {
        "scripts": ["background.js"],
        "type": "module"
      },
    "action": {
      "default_popup": "mainPopup.html",
      "default_icon": "hi.png",
      "default_title": "My Extension"
    },
    "permissions": [
        "activeTab",
        "storage"
      ]
  }